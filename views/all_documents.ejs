                                </div>

                                <div id="loader_container" class="w-100 h-100 text-center">
                                    <div class="loader"></div>
                                </div>

                                <div id="document_container" class="table-responsive table-responsive-data2 m-b-35">
                                    <div>
                                        <table class="table table-data2">
                                            <thead>
                                                <tr>
                                                    <th></th>
                                                    <th>Barcode</th>
                                                    <th>Document No.</th>
                                                    <th>Name</th>
                                                    <th>Description</th>
                                                    <th>Type</th>
                                                    <th>Urgency</th>
                                                    <th class="d-none">Duration</th>
                                                    <th>Remarks</th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody id="table_data">

                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                                <!-- Pager Container -->
                                <div id='pager_parent_container'>
                                    <div class="pager_container d-flex justify-content-center m-t-20"></div>
                                </div>

                                <div id="no_document_container" class="w-100 h-100 m-t-100 text-center d-none">
                                    <img src="images/no_document_icon.png" />
                                </div>

                                <!-- Copyright Container -->
                                <%- include footer.ejs %>

                                <!-- END DATA TABLE -->
                                <!-- Printed Barcode Container -->
                                <div id="barcode_container" class="d-none">
                                    <img id="barcode" style="float: bottom;" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="modal_track" class="modal fade" role="dialog">
        <div class="modal-dialog mxw-1300">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Document Tracking</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="table-responsive table-responsive-data2 m-b-35">
                        <table class="table table-data2">
                            <thead>
                                <tr>
                                    <th>Released To</th>
                                    <th>Received By</th>
                                    <th>IN</th>
                                    <th>Released By</th>
                                    <th>OUT</th>
                                    <th>Duration</th>
                                    <th>Remarks</th>
                                </tr>
                            </thead>
                            <tbody id="track_data">

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal -->
    <div id="modal_document" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content" style="width: 700px;">
                <div class="modal-header">
                    <h4 class="modal-title">Add Document</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">

                    <div id="error_container"></div>

                    <div class="row">
                        <div class="col-md-6">
                            <label>Name <span class="color-red">*</span></label>
                            <input id="modal_name" type="text" class="au-input au-input--full m-b-10 text-capitalize"
                                style="height: 36px;" />
                        </div>

                        <div class="col-md-6">
                            <label>Type <span class="color-red">*</span></label>
                            <input id="modal_type" type="text" class="au-input au-input--full m-b-10 text-capitalize"
                                style="height: 36px;" />
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <label>Document No. <span class="color-red">*</span></label>
                            <input id="modal_docno" type="text" class="au-input au-input--full m-b-10 text-uppercase"
                                style="height: 36px;" />
                        </div>

                        <div class="col-md-6">
                            <label>Urgency <span class="color-red">*</span></label>
                            <input id="modal_priority" type="text" class="au-input au-input--full m-b-10 text-capitalize"
                                style="height: 36px;" />
                        </div>
                    </div>

                    <label>Description <span class="color-red">*</span></label>
                    <textarea id="modal_description" class="au-input au-input--full m-b-10 text-capitalize"
                        placeholder="Write something here..."></textarea>

                    <label>Remarks </label>
                    <textarea id="modal_remarks" class="au-input au-input--full m-b-10 text-capitalize"
                        placeholder="Write something here..."></textarea>

                    <div class="modal-footer" style="justify-content: center;">
                        <button id="modal_cancel" type="button" class="au-btn au-btn-icon au-btn--blue au-btn--small"
                            data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </div>

