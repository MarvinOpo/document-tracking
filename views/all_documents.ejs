                                            <select id="document_filter" class="demo-default">
                                                <option value="">All Documents</option>
                                            </select>
                                        </div>
                                        <div class="rs-select2--light rs-select2--lg">
                                            <select id="type_filter" class="demo-default">
                                                <option value="">Any Type</option>
                                            </select>
                                        </div>

                                        <div class="rs-select2--light rs-select2--lg">
                                            <select id="docno_filter" class="demo-default">
                                                <option value="">Document No.</option>
                                            </select>
                                        </div>

                                        <div class="rs-select2--light rs-select2--300">
                                            <select id="general_filter" class="demo-default">
                                                <option value="">General Search</option>
                                            </select>
                                        </div>
                                    </div>
